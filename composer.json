{
    "name": "drupal/drupal",
    "description": "Project template for Drupal 8 sites.",
    "type": "project",
    "license": "GPL-2.0+",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "drupal/drupal-extension": "^3.2",
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "phpunit/phpunit": "~4.8",
        "behat/behat": "^3.0"
    },
    "require": {
        "consolidation/robo": "~1.2.4",
        "composer/installers": "^1.2",
        "drupal-composer/drupal-scaffold": "^2.0.0",
        "cweagans/composer-patches": "^1.6.0",
        "symfony/css-selector": "^3.4.0",
        "drupal/core": "^8.7",
        "drupal/embed": "^1.0",
        "drupal/entity_embed": "1.0.0-beta2",
        "drupal/media_entity": "^2.0",
        "drupal/config_update": "^1.1",
        "drupal/inline_entity_form": "^1.0",
        "drupal/metatag": "^1.0",
        "drupal/token": "^1.0",
        "drupal/pathauto": "^1.2.0",
        "drupal/views_infinite_scroll": "^1.1",
        "drupal/media_entity_document": "^1.0",
        "drupal/video_embed_field": "^2.0",
        "drupal/contact_storage": "^1.0",
        "drupal/diff": "^1.0",
        "drupal/entity_block": "^1.0.0-beta1",
        "drupal/search_api": "^1.0",
        "drupal/layout_plugin": "^1.0@alpha",
        "drupal/paragraphs": "^1.0",
        "drupal/focal_point": "^1.0",
        "drupal/menu_breadcrumb": "^1.0",
        "drupal/simple_sitemap": "^2.0",
        "drupal/shield": "^1.0",
        "drupal/addtoany": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/markup": "^1.0",
        "drupal/seckit": "^1.1",
        "drupal/redirect": "^1.3",
        "drupal/robotstxt": "^1.0",
        "drupal/samlauth": ">= 2.0",
        "drupal/autologout": "^1.0",
        "drupal/username_enumeration_prevention": "~1.0",
        "drupal/honeypot": "~1.0",
        "drupal/prlp": "1.*",
        "drupal/contact_storage_export": "^1.10",
        "drupal/entity_browser": "2.1.0",
        "drupal/link_attributes": "^1.0",
        "drupal/views_load_more": "1.x-dev",
        "drupal/better_exposed_filters": "^3.0@alpha",
        "drupal/admin_toolbar": "^1.23",
        "geoip2/geoip2": "~2.0",
        "cmfcmf/openweathermap-php-api": "^2.2",
        "drupal/twig_field_value": "^1.1",
        "drupal/page_manager": "^4.0@beta",
        "drupal/login_emailusername": "^1.1",
        "drupal/views_taxonomy_term_name_depth": "^3.3",
        "drupal/swiftmailer": "^1.0@beta",
        "drupal/mimemail": "^1.0@alpha",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/media_entity_actions": "^1.0@alpha",
        "drupal/media_entity_image": "^1.2",
        "drush/drush": "9.x",
        "drupal/backup_migrate": "^4.0",
        "drupal/advagg": "~3.0",
        "drupal/minifyhtml": "~1.0",
        "drupal/memcache_storage": "^1.0@beta",
        "drupal/memcache": "^2.0",
        "drupal/menu_export": "^1.2",
        "drupal/colorbutton": "^1.1",
        "drupal/panelbutton": "^1.1",
        "drupal/draggableviews": "^1.2",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/svg_image": "^1.9",
        "drupal/hreflang": "^1.1",
        "drupal/schema_metatag": "^1.3",
        "drupal/media_entity_instagram": "2.0.0-alpha2",
        "drupal/http_response_headers": "^2.0@alpha",
        "drupal/devel": "2.0",
        "twig/twig": "1.38.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ],
        "post-update-cmd": [
            "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
        ]
    },
    "config": {
        "bin-dir": "bin/"
    },
    "extra": {
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "drupal/core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "excludes": [
                ".htaccess",
                "robots.txt"
            ]
        }
    }
}
